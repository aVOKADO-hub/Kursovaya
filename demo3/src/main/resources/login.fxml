<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.geometry.*?>
<?import javafx.scene.control.*?>
<?import javafx.scene.layout.*?>

<VBox alignment="CENTER" fillWidth="false" prefHeight="200.0" prefWidth="240.0" xmlns="http://javafx.com/javafx/16" xmlns:fx="http://javafx.com/fxml/1" fx:controller="org.example.demo3.Login">
    <Label alignment="CENTER" text="Welcome in file manager">
      <VBox.margin>
         <Insets bottom="20.0" />
      </VBox.margin></Label>
    <Label alignment="CENTER" text="login" />
    <TextField fx:id="loginField" VBox.vgrow="ALWAYS">
      <VBox.margin>
         <Insets bottom="5.0" />
      </VBox.margin></TextField>
    <Label alignment="CENTER" text="password" />
    <TextField fx:id="passwordField" alignment="CENTER" VBox.vgrow="ALWAYS">
      <VBox.margin>
         <Insets bottom="20.0" />
      </VBox.margin></TextField>
    <Button fx:id="logIn" alignment="CENTER" onAction="#checkLogin" text="Log In" />
    <padding>
        <Insets bottom="5.0" left="5.0" right="5.0" top="5.0" />
    </padding>
    <padding>
        <Insets bottom="5.0" left="5.0" right="5.0" top="5.0" />
    </padding>
</VBox>
